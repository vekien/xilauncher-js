{
  "name": "XI Loader",
  "description": "XI Loader for private servers, converted to Electron",
  "author": "Josh Freeman <josh@viion.co.uk>",
  "version": "0.1.1",
  "license": "MIT",
  "main": "src/js/electron/electron.js",
  "build": {
    "appId": "game.ffxi.loader",
    "win": {
      "icon": "build/logo.ico"
    },
    "mac": {
      "category": "software"
    }
  },
  "scripts": {
    "encore-dev": "encore dev --watch",
    "encore-prod": "encore prod",
    "electron-dev": "electron . --dev",
