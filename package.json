    "prepack": "yarn run build",
    "pack": "electron-builder --dir",
    "predist": "yarn run build",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@symfony/webpack-encore": "^4.2.0",
    "com": "^0.0.5",
    "electron": "^22.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0",
    "regedit": "^5.1.1",
    "sass": "^1.57.0",
    "sass-loader": "^13.0.0",
    "sha1-file": "^3.0.0",
    "uuid": "^9.0.0"
  }
